decl
integer r;
integer fibrec(integer n);
enddecl



integer fibrec(integer n) {
decl
integer rvalue;
enddecl
begin
if (n==0) then
        rvalue=0;
else
    if (n == 1) then
        rvalue= 1;
    else
     rvalue = fibrec(n - 1) + fibrec(n - 2);
    endif;
endif;

return rvalue;
end

}


integer main( ){ // Main definition should always begin like this

decl

integer n,i ;

enddecl

begin

read (n);

i = 1;

while ( i <= n) do

write ( fibrec(i));

i = i + 1;

endwhile;

return 1;    // Any integer value may be returned

end

}