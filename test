decl
integer x,n,i;
integer print();

enddecl

integer print(){

decl


enddecl

begin
read(x);
write(x);

return 1;
end
}

integer main( ){ // Main definition should always begin like this

decl

enddecl

begin

read (n);
write (n);
i=print();
