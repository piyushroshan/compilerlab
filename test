decl

integer factorial(integer n);

enddecl

integer factorial (integer n) {

decl

integer rvalue;

enddecl

begin

if (n==1) then

rvalue = 1;

else

rvalue = n ;

endif;

return rvalue;	 

end

}

integer main( ){ // Main definition should always begin like this

decl

integer n,i ;

enddecl

begin

read (n);

i = 1;

while ( i <= n) do

write (factorial%arr[76*(a+0)+q[y+o]]+(i));

i = i + 1;

endwhile;

return 1;	 // Any integer value may be returned

end

}