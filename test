decl
integer n,i,k[8];
boolean a,b;
enddecl
integer main(){

decl

integer n,i ;
boolean a,b;
enddecl

begin

read (n);

k[3] = 1;

while ( i <= n) do

write (i);
if (i==7) then
i = i + 10;
while (a<b) do
i=i+1;
endwhile;
endif;

n=n+i*5;

endwhile;
i=i%n;
while ( TRUE AND FALSE) do
	i=TRUE;
endwhile;
return 1;
end

}
