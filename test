decl
	integer x,c ;
	boolean z,p,o,i;
	integer fact(integer n,a,b);
enddecl

integer fact(integer n,a,b)
{
decl
integer n;
enddecl
begin
 if(n>0)
 then
 write( fact(n-1));
 endif;
 return 1;
 end
}

